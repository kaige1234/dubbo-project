<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="dubbo-registry-consumer-server"/>
<!--    使用zookeeper时候需要使用改注册中心-->
<!--    <dubbo:registry address="zookeeper://192.168.1.9:2181" timeout="10000"/>-->

    <dubbo:registry address ="nacos://192.168.1.9:8848" timeout="10000"/>

    <dubbo:reference id="testApiService"
                     interface="cn.com.dubbo.registry.test.ITestRegistryService" version="1.0.0" />

</beans>